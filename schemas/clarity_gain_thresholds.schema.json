{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://owlume-engine/schemas/clarity_gain_thresholds.schema.json",
  "title": "Clarity Gain Thresholds",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "$schema": {
      "type": "string"
    },
    "spec": {
      "type": "string"
    },
    "version": {
      "type": "string",
      "pattern": "^\\d+\\.\\d+\\.\\d+$"
    },
    "thresholds": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "low": {
          "type": "number"
        },
        "medium": {
          "type": "number"
        },
        "high": {
          "type": "number"
        }
      }
    }
  }
}